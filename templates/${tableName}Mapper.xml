<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="${mapperPackage}.${title}Mapper">
  <resultMap id="BaseResultMap" type="${domainPackage}.${title}">
    <!--
      WARNING - @mbg.generated
    -->
	<#list table as abc>
	<#if abc.columnKey==true>
	<id column="${abc.columnName}" jdbcType="${abc.columnDbType}" property="${abc.columnName2}" />
	</#if>
	<#if abc.columnKey==false>
	<result column="${abc.columnName}" jdbcType="${abc.columnDbType}" property="${abc.columnName2}" />
	</#if>
	</#list>
    
  </resultMap>

  <select id="queryAll" resultMap="BaseResultMap">
    select t.*
    from ${tableName} t
    where 1 = 1
