      <when test="orderByStr != null and orderByStr != ''">
        ORDER BY ${r"${"}orderByStr${r"}"}
      </when>
      <otherwise>
        ORDER BY t.create_time DESC
      </otherwise>
    </choose>
  </sql>
</mapper>
